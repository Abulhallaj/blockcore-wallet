{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "blockcore-extension": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/blockcore-extension",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",

            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "customWebpackConfig": {
              "path": "./webpack.config.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/icons",
                "src/provider.js",
                "src/content.js",
                "src/manifest.json"
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "none"
            },
            "development": {
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/icons/blockcore",
                "src/provider.js",
                "src/content.js",
                "src/manifest.json"
              ],
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true,
              "customWebpackConfig": {
                "path": "./webpack-dev.config.ts"
              }
            },
            "coinvault": {
              "outputPath": "dist/coinvault-extension",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifest/coinvault/manifest.json"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/icons/coinvault",
                "src/provider.js",
                "src/content.js",
                {
                  "input": "src/manifest/coinvault/manifest.json",
                  "output": "src/manifest.json"
                }
              ],
              "outputHashing": "none"
            },
            "smartcityplatform": {
              "outputPath": "dist/smartcityplatform-extension",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "3mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "assets": [
                "src/favicon.ico",
                "src/assets",
                "src/icons/smartcityplatform",
                "src/provider.js",
                "src/content.js",
                {
                  "input": "src/manifest/smartcityplatform/manifest.json",
                  "output": "src/manifest.json"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                },
                {
                  "replace": "src/manifest.json",
                  "with": "src/manifest/smartcityplatform/manifest.json"
                }
              ],
              "outputHashing": "none"
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "options": {
            "port": 4201
          },
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "blockcore-extension:build:production"
            },
            "development": {
              "browserTarget": "blockcore-extension:build:development"
            },
            "coinvault": {
              "browserTarget": "blockcore-extension:build:coinvault"
            },
            "smartcityplatform": {
              "browserTarget": "blockcore-extension:build:smartcityplatform"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "blockcore-extension:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "blockcore-extension"
}